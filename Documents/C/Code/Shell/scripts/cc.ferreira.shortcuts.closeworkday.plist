<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>Label</key>
        <string>cc.ferreira.shortcuts.closeworkday</string>
        <key>ProgramArguments</key>
        <array>
                <string>sh</string>
                <string>-c</string>
                <string>shortcuts run "Close workday"</string>
        </array>
        <key>StartCalendarInterval</key>
        <dict>
                <key>Hour</key>
                <integer>23</integer>
                <key>Minute</key>
                <integer>00</integer>
        </dict>
        <key>StandardErrorPath</key>
        <string>/tmp/cc.ferreira.shortcuts.closeworkday.err</string>
        <key>StandardOutPath</key>
        <string>/tmp/cc.ferreira.shortcuts.closeworkday.out</string>
</dict>
</plist>